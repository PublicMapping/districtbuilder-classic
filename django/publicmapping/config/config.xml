                  <Field name="TRACTCE" type="name"/>
                  <Field name="GEOID" type="portable"/>
                  <Field name="COUNTYFP" type="tree" pos="0" width="3"/>
                  <Field name="GEOID" type="tree" pos="1" width="11"/>
              </Fields>
